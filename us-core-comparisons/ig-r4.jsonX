rg/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"apply-license"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"apply-publisher"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"active-tables"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"fmm-definition"},{"url":"value","valueString":"http://hl7.org/fhir/versions.html#maturity"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"propagate-status"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"excludelogbinaryformat"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"},{"extension":[{"url":"code","valueCode":"tabbed-snapshots"},{"url":"value","valueString":"true"}],"url":"http://hl7.org/fhir/tools/StructureDefinition/ig-parameter"}],"resource":[{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Device"}],"reference":{"reference":"Device/software-system-example"},"name":"Device - Software System Example","description":"Example of a Software System Device used to illustrate tooling, testing, or authoring system","exampleCanonical":"http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/device-softwaresystem-cqfm"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"MeasureReport"}],"reference":{"reference":"MeasureReport/testcase-example"},"name":"MeasureReport - Test Case Example","description":"Example of a MeasureReport illustrating a test case","exampleCanonical":"http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/test-case-cqfm"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Bundle"}],"reference":{"reference":"Bundle/sde-example-artifact-bundle"},"name":"Measure Content Bundle - Additional Supplemental Data Example","description":"Example of a Measure illustrating the use of supplemental data elements to gather additional information about a population","exampleBoolean":true},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"SearchParameter"}],"reference":{"reference":"SearchParameter/Library-contained-parameter-name"},"name":"Search Parameter - Library contained parameter name","description":"Example of a search parameter for a parameter name in a library","exampleBoolean":true},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"SearchParameter"}],"reference":{"reference":"SearchParameter/Library-expansion-identifier"},"name":"Search Parameter - Library expansion identifier","description":"Example of a search paramter for an expansion identifier in a library","exampleBoolean":true},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"SearchParameter"}],"reference":{"reference":"SearchParameter/ValueSet-keyword"},"name":"Search Parameter - Valuset Keyword","description":"Example of a search parameter for a keyword in a value set","exampleBoolean":true},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Bundle"}],"reference":{"reference":"Bundle/bundle-example"},"name":"Bundle example","description":"Example of a transaction bundle","exampleBoolean":true},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/capability-statement-cqfm"},"name":"CQFM Capability Statement","description":"The CQFM Capability Statement Profile allows systems to express their capabilities for handling CQFM profiles."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/cohort-measure-cqfm"},"name":"CQFM Cohort Measure","description":"The CQFM Cohort Measure Profile describes a cohort scoring measure"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/composite-measure-cqfm"},"name":"CQFM Composite Measure","description":"The CQFM Composite Measure Profile describes minimum expectations for a composite electronic Clinical Quality Measure (eCQM)"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/computable-library-cqfm"},"name":"CQFM Computable Library","description":"The CQFM Computable Library profile defines the must support elements for a computable library used to support eCQM logic"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/computable-measure-cqfm"},"name":"CQFM Computable Measure","description":"The CQFM Computable Measure Profile describes minimum expectations for a computable electronic Clinical Quality Measure (eCQM), including the use of a computable library, and the use of expressions or identifiers for all criteria."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/computable-valueset-cqfm"},"name":"CQFM Computable ValueSet","description":"Defines a computable value set as one that SHALL have an expression-based definition (i.e. a value set defined intensionally using expressions of the code systems involved) and MAY have an expansion included. The expression-based definition SHALL be represented in only one of three ways; using the compose element, using the expression extension, or using the rules-text extension to provide a step-by-step process for expanding the value set definition"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-aggregateMethod"},"name":"CQFM Aggregate Method","description":"Specifies which method to be used to aggregate the measure observation values."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-appliesTo"},"name":"CQFM Applies To","description":"Indicates the population that this stratifier should apply to."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-artifactComment"},"name":"CQFM Artifact Comment","description":"A comment containing additional documentation, a review comment, usage guidance, or other relevant information from a particular user."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-component"},"name":"CQFM Component","description":"Specifies an artifact this is a component of a composite artifact."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-compositeScoring"},"name":"CQFM Composite Scoring","description":"Indicates how the calculation is performed for the measure, including proportion, ratio, continuous-variable, and cohort. The value set is extensible, allowing additional measure scoring types to be represented. When present on a group, specifies the scoring to be used for this group, overriding the scoring specified for the overall measure."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-cqlOptions"},"name":"CQFM CQL Options","description":"Specifies a parameters (contained) resource that identifies the options for the CQL-to-ELM translator associated with the CQL content."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-criteriaReference"},"name":"CQFM Criteria Reference","description":"Specifies which population criteria is the input to an observation definition, or the initial population for a ratio numerator or denominator."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-defaultValue"},"name":"CQFM Default Value","description":"Provides a default value for a parameter definition."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-definitionTerm"},"name":"CQFM Definition Term","description":"Specifies a term and its associated definition."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-directReferenceCode"},"name":"CQFM Direct Reference Code","description":"Defines a code directly referenced by measure logic, as opposed to a value set. Direct reference codes may be the target of data requirements."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-effectiveDataRequirements"},"name":"CQFM Effective Data Requirements","description":"A module definition library that contains the effective data requirements for the measure (i.e. the calculated data requirements for the expressions referenced by the measure structure)."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-effectivePeriodAnchor"},"name":"CQFM Effective Period Anchor","description":"Specifies the anchor date for the effective period as defined by a starting date and a range."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-effectivePeriodDuration"},"name":"CQFM Effective Period Duration","description":"Specifies the reporting period for the measure."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-expansionParameters"},"name":"CQFM Expansion Parameters","description":"Specifies the expansion parameters that should be used to expand the ValueSet, or any ValueSets in the Library (or referenced by components of the Library)."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-fhirQueryPattern"},"name":"CQFM FHIR Query Pattern","description":"A FHIR Query URL pattern that corresponds to at least the data specified by the data requirement. If multiple FHIR Query URLs are present, they each contribute to the data specified by the data requirement (i.e. the union of the results of the FHIR Queries represents at least the complete data for the data requirement). In other words, the FHIR Query (or queries, taken together) MAY omit filters that are specified in the data requirement, such that the query would be expected to return more data than required, but it (or they, taken together) SHALL NOT return less data.                                                                                                This is not a resolveable URL, in that it will contain 1) No base canonical (i.e. itâ€™s a relative query), and 2) Parameters using tokens that are delimited using double-braces and the context parameters are dependent solely on the subjectType, according to the following: Patient: context.patientId, Practitioner: context.practitionerId, Organization: context.organizationId, Location: context.locationId, Device: context.deviceId."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-groupId"},"name":"CQFM Group Id","description":"Specifies the id of a particular group within the measure referenced by the related artifact resource."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-improvementNotation"},"name":"CQFM Improvement Notation","description":"Information on whether an increase or decrease in score is the preferred result (e.g., a higher score indicates better quality OR a lower score indicates better quality OR quality is within a range)."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-includeInReportType"},"name":"CQFM Include In Report Type","description":"Indicates what types of report the element on which it appears should be included in. If no values are specified, the default behavior is that elements are included in all report types"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-inputParameters"},"name":"CQFM Input Parameters","description":"Specifies the input parameters to the operation (such as a test case or data requirements operation)."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-isPrimaryCitation"},"name":"CQFM Is Primary Citation","description":"Specifies whether the related artifact is defining a primary citation for the measure (i.e. one that should appear in the narrative for the measure specification."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-isSelective"},"name":"CQFM Is Selective","description":"Allows a given data requirement to be identified as \"selective\", meaning that it can be used as an additive criteria to filter a population. A selective data requirement is guaranteed to define a subset (not necessarily proper) of the initial population of a measure. If multiple data requirements are marked selective, they all apply (i.e. AND semantics)."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-isTestCase"},"name":"CQFM Is Test Case","description":"Indicates whether this instance represents the definition of a test case."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-logicDefinition"},"name":"CQFM Logic Definition","description":"Defines a logic definition used by the measure."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-meaningfulMeasureArea"},"name":"CQFM Meaningful Measure Area","description":"Specifies the Meaningful Measures Area within the program that the measure covers or is related to."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-operation"},"name":"CQFM Operation","description":"Specifies the operation definition"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-partOf"},"name":"CQFM Part Of","description":"Specifies an overall quality program that this manifest or release is part of."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-populationBasis"},"name":"CQFM Population Basis","description":"The population basis specifies the type of elements in the population. For a subject-based measure, this is boolean (because the subject and the population basis are the same, and the population criteria define yes/no values for each individual in the population). For measures that have a population basis that is different than the subject, this element specifies the type of the population basis. For example, an encounter-based measure has a subject of Patient and a population basis of Encounter, and the population criteria all return lists of Encounters."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-programIdentifier"},"name":"CQFM Program Identifier","description":"Provides an identifier for the measure within the program."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-publicationDate"},"name":"CQFM Publication Date","description":"Specifies the publication date of the referenced artifacts."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-publicationStatus"},"name":"CQFM Publication Status","description":"Specifies the publication status of the referenced artifacts."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-qualityDomain"},"name":"CQFM Quality Domain","description":"Specifies the quality domain within the program that the measure covers or is related to."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-rateAggregation"},"name":"CQFM Rate Aggregation","description":"Describes how to combine the information calculated, based on logic in each of several populations, into one summarized result."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-riskAdjustment"},"name":"CQFM Risk Adjustment","description":"A description of the risk adjustment factors that may impact the resulting score for the measure and how they may be accounted for when computing and reporting measure results."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-scoring"},"name":"CQFM Scoring","description":"Indicates how the calculation is performed for the measure, including proportion, ratio, continuous-variable, and cohort. The value set is extensible, allowing additional measure scoring types to be represented. When present on a group, specifies the scoring to be used for this group, overriding the scoring specified for the overall measure."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-scoringUnit"},"name":"CQFM Scoring Unit","description":"Defines the expected units of measure for the measure score."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-softwaresystem"},"name":"CQFM Software System","description":"Specifies the software systems used in the packaging and testing of measure and library resources. Specifically to know which version of what software was used."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-supplementalDataGuidance"},"name":"CQFM Supplemental Data Guidance","description":"Provides additional guidance on the meaning and reporting of supplemental data elements included in the measure overall."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-supportedCqlVersion"},"name":"CQFM Supported CQL Version","description":"The version of CQL supported."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-testCaseDescription"},"name":"CQFM Test Case Description","description":"Provides a clear description of the test case."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-type"},"name":"CQFM Type","description":"Indicates whether the measure is used to examine a process, an outcome over time, a patient-reported outcome, or a structure measure such as utilization."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-valueFilter"},"name":"CQFM Value Filter","description":"Allows additional value-based filters to be specified as part of a data requirement."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:extension"}],"reference":{"reference":"StructureDefinition/cqfm-weight"},"name":"CQFM Weight","description":"What is the weight of the contribution of this measure to the overall score."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/cv-measure-cqfm"},"name":"CQFM Continuous Variable Measure","description":"The CQFM Continuous Variable Measure Profile describes a continuous variable scoring measure"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/device-softwaresystem-cqfm"},"name":"CQFM Device","description":"The SoftwareSystem device profile is used to capture software details about the various systems used in authoring, testing, packaging, or evaluation of a knowledge artifact"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/executable-library-cqfm"},"name":"CQFM Executable Library","description":"The CQFM Execuable Library profile defines the must support elements for an executable library used to support eCQM logic"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/executable-measure-cqfm"},"name":"CQFM Executable Measure","description":"The CQFM Executable Measure Profile describes minimum expectations for an executable electronic Clinical Quality Measure (eCQM), including a reference to an executable library, and restricted to the use of identifier-based expressions only for all criteria."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/executable-valueset-cqfm"},"name":"CQFM Executable ValueSet","description":"Defines an executable value set as one that SHALL have an expansion included, as well as a usage warning indicating the expansion is a point-in-time snapshot and must be maintained over time for production usage. The value set expansion specifies the timestamp when the expansion was produced, SHOULD contain the parameters used for the expansion, and SHALL contain the codes that are obtained by evaluating the value set definition. If this is ONLY an executable value set, a computable definition of the value set must be obtained to compute the updated expansion."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/library-cqfm"},"name":"CQFM Library","description":"The CQFM Library profile defines the minimum expectations for a library used to distribute eCQM logic"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/measure-cqfm"},"name":"CQFM Measure","description":"The CQFM Measure Profile describes minimum expectations for defining an electronic Clinical Quality Measure (eCQM)"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/modelinfo-library-cqfm"},"name":"CQFM Model Info Library","description":"The CQFM Model Information Library profile defines the must support elements for a computable Model Definition library used to support declaration and distribution of CQL ModelInfo such as the FHIR Model Info Library"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/module-definition-library-cqfm"},"name":"CQFM Module Definition Library","description":"The CQFM Module Definition Library profile defines the must support elements for a library that communicates the effective data requirements and dependencies of an artifact (or set of artifacts). This is most commonly used as the result of a $data-requirements operation, but could also be persisted statically as a way to persist the calculation of effective data requirements for a specific artifact and input parameters. This provides a filtered view of elements, definitions, codes and value sets required for the measure."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/proportion-measure-cqfm"},"name":"CQFM Proportion Measure","description":"The CQFM Proportion Measure Profile describes a proportion scoring measure"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/publishable-codesystem-cqfm"},"name":"CQFM Publishable CodeSystem","description":"Defines the expectations for publishable code systems used with quality measure specifications"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/publishable-library-cqfm"},"name":"CQFM Publishable Library","description":"The CQFM Publishable Library profile defines the must support elements in a library used to support eCQM logic"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/publishable-measure-cqfm"},"name":"CQFM Publishable Measure","description":"The CQFM Publishable Measure profile defines the minimum expectations to enable appropriate content management, discovery, and utilization of published electronic Clinical Quality Measures (eCQMs)"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/publishable-valueset-cqfm"},"name":"CQFM Publishable ValueSet","description":"Defines the expectations for publishable value sets used in quality measurement specifications"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/quality-program-cqfm"},"name":"CQFM Quality Program","description":"Defines and supports the definition of Quality Programs"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/ratio-measure-cqfm"},"name":"CQFM Ratio Measure","description":"The CQFM Ratio Measure Profile describes a ratio scoring measure"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"StructureDefinition:resource"}],"reference":{"reference":"StructureDefinition/test-case-cqfm"},"name":"CQFM Test Case","description":"The CQFM Test Case Profile describes expectations for the definition and exchange of a measure test case, including input data and parameters, as well as expected outcome"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CapabilityStatement"}],"reference":{"reference":"CapabilityStatement/authoring-measure-repository"},"name":"CQF Measures Authoring Measure Repository Capability Statement","description":"Capability statement for a repository service supporting additional authoring and content workflow capabilities for FHIR-based measure specifications above the basic ShareableMeasureRepository."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CapabilityStatement"}],"reference":{"reference":"CapabilityStatement/measure-calculation-service-example"},"name":"Measure Calculation Service Capability Statement Example","description":"Basic conformance statement for a Measure Calculation Service. A server can support more fucntionality    than defined here, but this is the minimum amount","exampleCanonical":"http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/capability-statement-cqfm"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CapabilityStatement"}],"reference":{"reference":"CapabilityStatement/measure-terminology-service"},"name":"Measure Terminology Service Capability Statement","description":"Capability statement for a terminology service supporting measure authoring and evaulation use cases. A server can support more functionality than defined here, but this defines the minimum expectations."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CapabilityStatement"}],"reference":{"reference":"CapabilityStatement/publishable-measure-repository"},"name":"CQF Measures Publishable Measure Repository Capability Statement","description":"Capability statement for a repository service supporting additional publishing capabilities for FHIR-based measure specifications above the basic ShareableMeasureRepository."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CapabilityStatement"}],"reference":{"reference":"CapabilityStatement/shareable-measure-repository"},"name":"CQF Measures Shareable Measure Repository Capability Statement","description":"Capability statement for a repository service supporting minimum required capabilities to share FHIR-based measure specifications. See the Publishable and Authoring Measure Repository capability statements for more comprehensive support for publishing and authoring workflows."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"CodeSystem"}],"reference":{"reference":"CodeSystem/publishable-example"},"name":"SNOMED\u00a0CT International Edition","description":"This is an example code system, based on the SNOMED CT Code System to illustrate a publishable code system resource. SNOMED CT is a core clinical healthcare terminology that contains concepts with unique meanings and formal logic based definitions organized into hierarchies.","exampleCanonical":"http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/publishable-codesystem-cqfm"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/aggregate-method"},"name":"Aggregate Method mapping","description":"The mapping of FHIR aggregate method codes to the corresponding HL7 V3 code used in HQMF for each type of aggregate method"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/composite-measure-scoring"},"name":"Composite Measure Scoring mapping","description":"The mapping of FHIR composite measure scoring codes to the corresponding HL7 V3 code used in HQMF for each type of composite measure scoring"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/data-usages"},"name":"Data Usage Mapping","description":"The mapping of FHIR measure data usage codes to the corresponding HL7 V3 code used in HQMF for each of the measure data usages"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/improvement-notations"},"name":"Improvement Notation Mapping","description":"The mapping of FHIR improvement notation codes to the corresponding HL7 V3 code used in HQMF for each of the improvement notations"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/measure-populations"},"name":"Measure Population criteria mapping","description":"The mapping of FHIR measure population criteria codes to the corresponding HL7 V3 code used in HQMF for each of the measure population criteria components"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/measure-scoring"},"name":"Measure Scoring mapping","description":"The mapping of FHIR measure scoring codes to the corresponding HL7 V3 code used in HQMF for each type of measure scoring"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"ConceptMap"}],"reference":{"reference":"ConceptMap/measure-types"},"name":"Measure Type mapping","description":"The mapping of FHIR measure type codes to the corresponding HL7 V3 code used in HQMF for each of the measure type"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Library"}],"reference":{"reference":"Library/AdultOutpatientEncounters"},"name":"Adult Outpatient Encounters Common Library","description":"This library is used as an example in the FHIR Quality Measure Implementation Guide","exampleCanonical":"http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/library-cqfm"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Library"}],"reference":{"reference":"Library/AdvancedIllnessandFrailtyExclusion"},"name":"Advanced Illness and Frailty Exclusion Library","description":"Advanced Illness and Frailty Exclusion Example Library"},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Library"}],"reference":{"reference":"Library/BCSComponent"},"name":"BCS Component Library","description":"This library is the primary measure library for the Breast Cancer Screening component of the composite measure examples in this implementation guide."},{"extension":[{"url":"http://hl7.org/fhir/tools/StructureDefinition/resource-information","valueString":"Library"}],"reference":{"reference":"Library/CCSComponent"},"name":"CCS Component Library","descripti